<?xml version="1.0" encoding="utf-8"?>
<Configuration monitorInterval="30">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%5p %d{yyyy-MM-dd HH:mm:ss} [%t] %c{3}:%L - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="INFO">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>
</Configuration>